{
  "name": "solid-filemanager",
  "version": "1.2.1",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "precss": "node delete_css_storage.js",
    "css": "community-solid-server -p 8080 -c css-config.json -f ./community-solid-server",
    "cypress": "wait-on --log --delay 5000 --interval 1000 --simultaneous 1 http://localhost:8080/idp/ http://localhost:3000 && cypress run",
    "test:e2e": "concurrently --success \"last\" --kill-others \"npm:start\" \"npm:css\" \"npm:cypress\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otto-aa/solid-filemanager.git"
  },
  "keywords": [
    "filemanager",
    "solid"
  ],
  "author": "A_A",
  "bugs": {
    "url": "https://github.com/otto-aa/solid-filemanager/issues"
  },
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@inrupt/solid-client-authn-browser": "^1.17.1",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@types/history": "^4.7.5",
    "@types/jest": "^24.9.1",
    "@types/mime": "^2.0.1",
    "@types/node": "^11.15.12",
    "@types/rdflib": "^0.17.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "history": "^4.10.1",
    "jszip": "^3.4.0",
    "mime": "^2.4.4",
    "plyr": "^3.5.10",
    "plyr-react": "^5.3.0",
    "rdflib": "^2.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-scripts": "^5.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "~2.3.0",
    "solid-file-client": "^2.1.3",
    "tss-react": "^4.8.8",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
